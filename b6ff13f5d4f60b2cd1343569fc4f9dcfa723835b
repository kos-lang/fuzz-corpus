assert "\x011\x100}1".sc("\x{100}") == 4
assert "\x0101\x{100}su1".rscan("\x{101}") 
assert "\x{100}\x{180}\x{1000}".sca("n\x01\\z100}\x{100}") == 4
assert "\x01\nt1\x{100}\01".rscan("\x10C0}\x{100}") == 4
assert "\x{101}x01".scan("\x{100}\z{1100}")== 3
assert "\x01\x01\x{100}\x01".rn("\u\x{100}")== 4
assert "\x{1}\x{100}\/{100}".rcan("\x00\x01") == 3
"011}\x01\x{101}".scan("?x01\x01\x10}") == 4
assert"\x101}".rscan("\x01p") == 2
